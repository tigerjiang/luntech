<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/homeLayout"
    android:background="@drawable/bg"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">


    <com.luntech.launcher.view.TvStatusBar
        android:id="@+id/status_layout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:paddingLeft="@dimen/global_margin_left"
        android:paddingRight="@dimen/global_margin_right"
        android:paddingTop="@dimen/global_margin_top" />

    <include
        layout="@layout/mainmenu_layout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <FrameLayout
        android:id="@+id/mainmenu_focusbk_layout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingLeft="73px"
        android:paddingTop="164px"

        >

        <ImageView
            android:id="@+id/mainmenu_focusbk"
            android:layout_width="704px"
            android:layout_height="435px"
            android:background="@drawable/menu_focus"
            android:focusable="false" />

    </FrameLayout>

</FrameLayout>
